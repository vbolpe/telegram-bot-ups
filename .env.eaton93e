# Configuración del Bot de Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Configuración SNMP v3
SNMP_HOST=192.168.1.100
SNMP_PORT=161
SNMP_USER=snmpuser
SNMP_AUTH_PROTOCOL=SHA
SNMP_AUTH_PASSWORD=auth_password_here
SNMP_PRIV_PROTOCOL=AES
SNMP_PRIV_PASSWORD=priv_password_here
SNMP_SECURITY_LEVEL=authPriv

# OIDs específicos para Eaton 93E 30kVA
# Estos OIDs son para la serie Eaton 93E con Network Management Card (NMC)

# Estado general de la UPS
OID_UPS_STATUS=1.3.6.1.4.1.534.1.4.1.0
# 1=unknown, 2=onLine, 3=onBattery, 4=onBoost, 5=onSleep, 6=off, 7=rebooting, 8=onBypass

# Estado de la batería
OID_UPS_BATTERY_STATUS=1.3.6.1.4.1.534.1.2.1.0
# 1=unknown, 2=batteryNormal, 3=batteryLow, 4=batteryDepleted

# Carga de la batería (porcentaje)
OID_UPS_BATTERY_CAPACITY=1.3.6.1.4.1.534.1.2.4.0

# Tiempo restante de batería (segundos)
OID_UPS_BATTERY_RUNTIME=1.3.6.1.4.1.534.1.2.5.0

# Voltaje de entrada (en 0.1V - dividir por 10)
OID_UPS_INPUT_VOLTAGE=1.3.6.1.4.1.534.1.3.1.0

# Frecuencia de entrada (en 0.1Hz - dividir por 10)
OID_UPS_INPUT_FREQUENCY=1.3.6.1.4.1.534.1.3.2.0

# Voltaje de salida (en 0.1V - dividir por 10)
OID_UPS_OUTPUT_VOLTAGE=1.3.6.1.4.1.534.1.4.2.0

# Frecuencia de salida (en 0.1Hz - dividir por 10)
OID_UPS_OUTPUT_FREQUENCY=1.3.6.1.4.1.534.1.4.3.0

# Carga de salida (porcentaje)
OID_UPS_OUTPUT_LOAD=1.3.6.1.4.1.534.1.4.4.1.3.1

# Corriente de salida (en 0.1A - dividir por 10)
OID_UPS_OUTPUT_CURRENT=1.3.6.1.4.1.534.1.4.4.1.2.1

# Potencia de salida (en Watts)
OID_UPS_OUTPUT_POWER=1.3.6.1.4.1.534.1.4.4.1.4.1

# Temperatura ambiente (en Celsius)
OID_UPS_TEMPERATURE=1.3.6.1.4.1.534.1.6.1.0

# Información del sistema (opcional pero útil)
OID_UPS_MODEL=1.3.6.1.4.1.534.1.1.2.0
OID_UPS_FIRMWARE=1.3.6.1.4.1.534.1.1.3.0
OID_UPS_SERIAL=1.3.6.1.4.1.534.1.1.1.0

# Alarmas activas
OID_UPS_ALARMS=1.3.6.1.4.1.534.1.7.1.0

# Bypass status (importante para 93E)
OID_UPS_BYPASS_VOLTAGE=1.3.6.1.4.1.534.1.5.1.0
OID_UPS_BYPASS_FREQUENCY=1.3.6.1.4.1.534.1.5.2.0

# Configuración de monitoreo
CHECK_INTERVAL_SECONDS=60
DAILY_REPORT_TIME=09:00

# Zona horaria
TZ=America/Argentina/Buenos_Aires
