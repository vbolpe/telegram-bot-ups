# OIDs VALIDADOS para Eaton 93E 30kVA (Serie: 4K42SLXX01)
# Estos OIDs han sido probados y funcionan correctamente

# Estado general de la UPS
OID_UPS_STATUS=1.3.6.1.4.1.534.1.4.1.0
# Valores: 14=Power Saving Mode (ECOnversion), 2=Online, 3=On Battery, 8=Bypass

# Estado de la batería
OID_UPS_BATTERY_STATUS=1.3.6.1.4.1.534.1.2.1.0
# Valores: 2=Normal, 3=Low, 4=Depleted, 8=Charging

# Carga de la batería (porcentaje)
OID_UPS_BATTERY_CAPACITY=1.3.6.1.4.1.534.1.2.4.0

# Tiempo restante de batería (segundos)
OID_UPS_BATTERY_RUNTIME=1.3.6.1.4.1.534.1.2.5.0

# Frecuencia de entrada (en 0.1Hz - dividir por 10)
OID_UPS_INPUT_FREQUENCY=1.3.6.1.4.1.534.1.3.1.0
# Devuelve: 500 = 50.0 Hz

# Voltaje de entrada Fase 1 (230V fase-neutro en sistema 400V trifásico)
OID_UPS_INPUT_VOLTAGE=1.3.6.1.4.1.534.1.3.4.1.2.1
# Devuelve: 235V (directo, sin escalar)

# Frecuencia de salida (en 0.1Hz - dividir por 10)
OID_UPS_OUTPUT_FREQUENCY=1.3.6.1.4.1.534.1.4.2.0
# Devuelve: 500 = 50.0 Hz

# Voltaje de salida Fase 1
OID_UPS_OUTPUT_VOLTAGE=1.3.6.1.4.1.534.1.4.4.1.2.1
# Devuelve: 220V (directo, sin escalar)

# Carga de salida (porcentaje) - Fase 1
OID_UPS_OUTPUT_LOAD=1.3.6.1.4.1.534.1.4.4.1.3.1

# Corriente de salida Fase 1 (en Amperios)
OID_UPS_OUTPUT_CURRENT=1.3.6.1.4.1.534.1.4.4.1.1.1

# Potencia de salida Fase 1 (en Watts)
OID_UPS_OUTPUT_POWER=1.3.6.1.4.1.534.1.4.4.1.4.1
# Devuelve: 900W (directo)

# Temperatura ambiente (en Celsius)
OID_UPS_TEMPERATURE=1.3.6.1.4.1.534.1.6.1.0

# Voltaje de bypass
OID_UPS_BYPASS_VOLTAGE=1.3.6.1.4.1.534.1.5.1.0

# Número de alarmas activas
OID_UPS_ALARMS=1.3.6.1.4.1.534.1.7.1.0

# Información del sistema (opcional)
OID_UPS_MODEL=1.3.6.1.4.1.534.1.1.2.0
OID_UPS_FIRMWARE=1.3.6.1.4.1.534.1.1.3.0
OID_UPS_SERIAL=1.3.6.1.4.1.534.1.1.1.0

